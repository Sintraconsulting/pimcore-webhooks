{
    "name": "sintra/pimcore-webhooks",
    "description": "A Pimcore's bundle for implementing webhooks",
    "license": "GPL-3.0+",
    "type": "library",
   
  "config": {
    "discard-changes": true
  },
  "autoload": {
    "psr-4": {
      "WebHookBundle\\": "./"
    },
    "exclude-from-classmap": [
      "vendor/phpunit"
    ]
  },
  "require": {
    "pimcore/pimcore": "^6.8 || ^10.0"
  },
    "extra": {
        "pimcore": {
            "bundles": [
                "WebHookBundle\\WebHookBundle"
            ]
        }
    },
  "require-dev": {
    "symfony/phpunit-bridge": "*"
  }
}
